<h1>List of Users</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
    </tr>
  </thead>
<% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
    </tr>
  <% end %>
</table>

<br>
<% link_to 'New User', new_user_path %>
